/* components/nav-title/nav-title.wxss */
view{box-sizing: border-box;}
.nav-box{position: fixed;z-index: 999999;top: 0;left: 0;right: 0;background: #cd3;overflow: hidden;}
.hover-active{position: relative;z-index: 0;}
.hover-active::before{content: '';position: absolute;top: 0;left: 0;right: 0;bottom: 0;background: rgba(0, 0, 0, .2);z-index: 0;}
.title{line-height: 46px;display: flex;height: 46px;}
.col{flex: 1;text-align: center;font-size: 36rpx}
.ellipsis-1{text-overflow: ellipsis;overflow: hidden;white-space: nowrap;}
.container{position: absolute;left: 0;right: 0;bottom: 0;}
@font-face{
  font-family: "iconfont";
  src: url("data:application/octet-stream;base64,") format('truetype');
}
.iconfont, .icon__back {
  font-family: "iconfont" !important;
  font-size: 22px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.icon__back{padding: 0 10px;}
.icon__back:before {content: "\e611";position: relative;z-index: 110;}